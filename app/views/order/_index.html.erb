<div class="container_cart">
    <div class="row">     
      <div class="main col-12">
        <table class="table cart table-hover table-colored">
      		<thead>
  	            <tr>
  	              <th>Date</th>
  	              <th>Items</th>
  	              <th>Total Price</th>
  	              <th>Shipping status</th>
  	            </tr>
            	</thead>
              <tbody>                
  			<% current_artisan.orders.each do |order| %>
  				<tr class='order-details' id=<%order.id%> >
  					<td><%= order.created_at %></td>
  					<td><%= order_count_items(order.id) %></td>
  					<td>order.total_price</td>
  					<td>order.status</td>
  				</tr>
  			<% end %>
  			</tbody>
  		</table>
    </div>
  </div>
</div>